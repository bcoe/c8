{
  "name": "c8",
  "version": "2.0.0",
  "description": "collect test coverage using v8's inspector",
  "main": "index.js",
  "bin": "./bin/c8.js",
  "scripts": {
    "bundle": "bundle-dependencies update",
    "test": "nyc mocha ./test/*.js",
    "posttest": "standard",
    "release": "standard-version"
  },
  "c8": {
    "exclude": [
      "test/*.js"
    ]
  },
  "standard": {
    "ignore": [
      "test/fixtures"
    ]
  },
  "keywords": [
    "coverage",
    "v8",
    "test",
    "istanbul",
    "profiler",
    "inspector"
  ],
  "engines": {
    "node": ">=8.8.1"
  },
  "author": "Ben Coe <ben@npmjs.com>",
  "license": "ISC",
  "dependencies": {
    "find-up": "^3.0.0",
    "foreground-child": "^1.5.6",
    "istanbul-lib-coverage": "^2.0.1",
    "istanbul-lib-report": "^2.0.1",
    "istanbul-reports": "^2.0.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2",
    "signal-exit": "^3.0.2",
    "spawn-wrap": "^1.4.2",
    "test-exclude": "^5.0.0",
    "uuid": "^3.1.0",
    "v8-to-istanbul": "^1.2.0",
    "yargs": "^12.0.1",
    "yargs-parser": "^10.1.0"
  },
  "devDependencies": {
    "bundle-dependencies": "^1.0.2",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "standard": "^11.0.1",
    "standard-version": "^4.2.0"
  },
  "bundledDependencies": [
    "find-up",
    "foreground-child",
    "istanbul-lib-coverage",
    "istanbul-lib-report",
    "istanbul-reports",
    "mkdirp",
    "rimraf",
    "signal-exit",
    "spawn-wrap",
    "test-exclude",
    "uuid",
    "v8-to-istanbul",
    "yargs",
    "yargs-parser"
  ]
}
